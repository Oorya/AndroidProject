<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.finalproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AuthActivities.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AuthActivities.Register" />
        <activity android:name=".TicketActivities.CreateTicket" />
        <activity android:name=".TicketActivities.TicketList" />
        <activity android:name=".TicketActivities.DetailsTicket" />
        <activity android:name=".TicketActivities.ChatTicket" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".TicketActivities.AdminEntry" />
        <activity android:name=".AuthActivities.LoginGoogle" />
        <activity android:name=".AuthActivities.SignupActivity" />
        <activity android:name=".AuthActivities.ResetPasswordActivity" />
        <activity android:name=".AuthActivities.MainActivity" />
        <activity android:name=".TicketActivities.AccountSettings"></activity>
    </application>

</manifest>